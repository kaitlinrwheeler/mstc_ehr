<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] myEHR</title>
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/EHRApplication.styles.css" asp-append-version="true" />
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=BIZ+UDPGothic">

    </head>
    <partial name="_landingNav" />
    <body id="landingBody">
        <main role="main" class="pb-3 pt-5">
            @RenderBody()
            <partial name="_landingFooter" />
        </main>

        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>

        <!-- Font Awesome imports. -->
        <script src="~/fontawesome-free-6.5.1-web/js/brands.js"></script>
        <script src="~/fontawesome-free-6.5.1-web/js/solid.js"></script>
        <script src="~/fontawesome-free-6.5.1-web/js/fontawesome.js"></script>

        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>